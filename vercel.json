{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "functions": {
      "api/cron/*.js": {
        "maxDuration": 300
      }
    },
    "crons": [
      {
        "path": "/api/cron/enrich-leads",
        "schedule": "* * * * * *"
      }
    ]
  }